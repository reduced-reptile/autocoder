{
  "name": "autocoder-package",
  "version": "1.0.0",
  "description": "An automated coder for solving contest challenges",
  "keywords": ["Contest", "Automated"],
  "files": ["built/**/*"],
  "main": "built/index.js",
  "types": "built/index.d.ts",
  "bugs": "https://github.com/reduced-reptile/autocoder/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/reduced-reptile/autocoder.git"
  },
  "scripts": {
    "test": "jest",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare" : "npm run build",
    "prepublishOnly" : "npm test && npm run lint",
    "preversion" : "npm run lint",
    "version" : "npm run format && git add -A src",
    "postversion" : "git push && git push --tags"
  },
  "engines": {
    "node": ">= 12.13.1",
    "npm": ">= 6.12.1"
  },
  "browserslist": [
    "last 1 node version"
  ],
  "author": "Raymond Mendelovits",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@types/jest": "^24.0.24",
    "@types/node": "^13.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "js-yaml": "^3.13.1"
  }
}
